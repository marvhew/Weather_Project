package com.example.weather_project;

import org.json.JSONException;
import org.json.JSONObject;

public class Temperature {
	long day;
	long min;
	long max;
	long night;
	long eve;
	long morn;
	
	public Temperature(JSONObject object)
	{
		try {
			day = object.getLong("day");
			min = object.getLong("min");
			max = object.getLong("max");
			night = object.getLong("night");
			eve = object.getLong("eve");
			morn = object.getLong("morn");
		} catch (JSONException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}